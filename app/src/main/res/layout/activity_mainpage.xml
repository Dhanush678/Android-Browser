            </ImageView>
            <ImageView
                android:id="@+id/medium"

                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="230dp"
                android:layout_marginTop="70dp"
                android:src="@drawable/medium">

            </ImageView>
            <ImageView
                android:id="@+id/python"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="230dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/python">

            </ImageView>

            <ImageView
                android:id="@+id/hack"

                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="300dp"
                android:src="@drawable/hack">

            </ImageView>

            <ImageView
                android:id="@+id/github"

                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="300dp"
                android:layout_marginTop="70dp"
                android:src="@drawable/git">

            </ImageView>
            <ImageView
                android:id="@+id/javalogo"

                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="300dp"
                android:layout_marginTop="70dp"
                android:src="@drawable/java">

            </ImageView>
            <ImageView
                android:id="@+id/cpp"

                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="300dp"
                android:src="@drawable/cc">

            </ImageView>



        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="350dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/asar"
            android:textSize="18dp"
            android:text="Entertainment Sites">
        </TextView>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="380dp"

            android:layout_height="190dp">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"

                android:layout_marginLeft="15dp"
                android:src="@drawable/d">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="70dp"

                android:layout_marginLeft="15dp"
                android:src="@drawable/movies">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="140dp"

                android:layout_marginLeft="15dp"
                android:src="@drawable/quora">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="90dp"
                android:src="@drawable/hd">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="70dp"
                android:layout_marginLeft="90dp"
                android:src="@drawable/insta">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="90dp"
                android:src="@drawable/reddit">

            </ImageView>





            <ImageView
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginTop="1dp"
                android:layout_marginStart="160dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/prime">

            </ImageView>
            <ImageView
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginTop="70dp"
                android:layout_marginStart="160dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/face">

            </ImageView>
            <ImageView
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginTop="140dp"
                android:layout_marginStart="160dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/amazon">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="230dp"
                android:src="@drawable/net">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="70dp"
                android:layout_marginLeft="230dp"
                android:src="@drawable/whatsapp">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="230dp"
                android:src="@drawable/zomato">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="300dp"
                android:src="@drawable/bros">

            </ImageView>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
